{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ],

    "crons": [
        {
          "path": "/main.py",
          "schedule": "* * * * *"
        }
      ]
  }