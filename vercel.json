{
    "version": 2,
    "crons": [
        {
          "path": "/main",
          "schedule": "* * * * *"
        }
      ],
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ]

  }